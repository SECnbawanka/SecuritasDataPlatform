		SYSTEM_CONTRACT_TYPE_ID NUMBER(5,0),
		CENTRAL_USER VARCHAR(60),
		CENTRAL_COMPANY VARCHAR(60),
		ODS_SRC_SYSTEM_ID NUMBER(38,0),
		ODS_CREATEDBY VARCHAR(255) NOT NULL DEFAULT CURRENT_USER(),
