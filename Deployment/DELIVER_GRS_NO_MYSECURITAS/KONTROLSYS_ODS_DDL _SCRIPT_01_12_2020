CREATE OR REPLACE DATABASE ODS_KONTROLSYS_NO;
DROP SCHEMA ODS_KONTROLSYS_NO.PUBLIC;
CREATE OR REPLACE SCHEMA ODS_KONTROLSYS_NO.DBO;

USE SCHEMA ODS_KONTROLSYS_NO.DBO

